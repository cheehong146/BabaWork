<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="768.0" prefWidth="1366.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.SimulationPageController">
  <children>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <HBox alignment="CENTER_LEFT" prefHeight="100.0" spacing="15.0" HBox.hgrow="ALWAYS">
               <children>
                  <Button fx:id="btnBack" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#btnBackPressed" prefHeight="86.0" prefWidth="86.0" text="Back" />
                  <Button fx:id="btnRetry" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#btnRetryPressed" prefHeight="86.0" prefWidth="86.0" text="Retry" />
               </children>
               <padding>
                  <Insets left="25.0" />
               </padding>
            </HBox>
            <HBox alignment="CENTER_RIGHT" prefHeight="100.0" spacing="15.0" HBox.hgrow="ALWAYS">
               <children>
                  <Button fx:id="btnRunCode" mnemonicParsing="false" onAction="#btnRunCodePressed" prefHeight="75.0" text="RUN CODE" />
                  <Button fx:id="btnExit" mnemonicParsing="false" onAction="#btnExitPressed" prefHeight="75.0" text="EXIT SIMULATION" />
               </children>
               <padding>
                  <Insets right="25.0" />
               </padding>
            </HBox>
         </children>
      </HBox>
      <SplitPane dividerPositions="0.75" prefHeight="630.0">
         <items>
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="100.0">
               <children>
                  <TextArea fx:id="txtBoxContents" onDragDone="#textBoxContentsDragged" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
               </children>
            </AnchorPane>
            <ScrollPane prefHeight="1080.0" prefWidth="200.0" style="-fx-background-color: black;">
               <content>
                  <VBox alignment="TOP_CENTER" minWidth="300.0" spacing="25.0">
                     <children>
                        <Button fx:id="btnBack1" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#btnBackPressed" prefHeight="86.0" prefWidth="86.0" text="TEST" />
                        <Button minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" prefHeight="86.0" prefWidth="86.0" text="TEST" />
                        <Button minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" prefHeight="86.0" prefWidth="86.0" text="TEST" />
                        <Button minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" prefHeight="86.0" prefWidth="86.0" text="TEST" />
                        <Button minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" prefHeight="86.0" prefWidth="86.0" text="TEST" />
                        <Button minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" prefHeight="86.0" prefWidth="86.0" text="TEST" />
                        <Button minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" prefHeight="86.0" prefWidth="86.0" text="TEST" />
                        <Button minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" prefHeight="86.0" prefWidth="86.0" text="TEST" />
                        <Button minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" prefHeight="86.0" prefWidth="86.0" text="TEST" />
                     </children>
                  </VBox>
               </content>
            </ScrollPane>
         </items>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </SplitPane>
  </children>
</VBox>
