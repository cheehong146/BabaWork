<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.SimulationChoiceController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <BorderPane layoutX="142.0" layoutY="27.0" prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
               <center>
                  <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="25.0" BorderPane.alignment="CENTER">
                     <children>
                        <Label text="Pick a Simulation:" textAlignment="CENTER">
                           <font>
                              <Font size="40.0" />
                           </font>
                        </Label>
                        <Button fx:id="btnEPayment" mnemonicParsing="false" text="E-Payment" id = "btnEPayment" onAction="#btnEPaymentClick">
                           <font>
                              <Font size="30.0" />
                           </font>
                        </Button>
                        <Button fx:id="btnWebsite" mnemonicParsing="false" text="Website" id = "btnWebsite" onAction="#btnWebsiteClick">
                           <font>
                              <Font size="30.0" />
                           </font>
                        </Button>
                     </children>
                  </VBox>
               </center>
            </BorderPane>
      </children>
    </AnchorPane>
  </children>
</VBox>
