<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.WebsiteController">
  <children>
      <BorderPane prefHeight="200.0" prefWidth="200.0">
         <top>
            <Button fx:id="btnBack" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#onBackPressed" prefHeight="86.0" prefWidth="86.0" text="Back" BorderPane.alignment="TOP_LEFT">
               <BorderPane.margin>
                  <Insets left="50.0" top="50.0" />
               </BorderPane.margin>
            </Button>
         </top>
         <center>
            <TextArea fx:id="txtAreaContents" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="20.0" BorderPane.alignment="CENTER">
               <BorderPane.margin>
                  <Insets left="200.0" right="200.0" top="20.0" />
               </BorderPane.margin>
            </TextArea>
         </center>
         <bottom>
            <Button fx:id="btnContinue" mnemonicParsing="false" onAction="#onContinuePressed" prefHeight="48.0" prefWidth="256.0" text="CONTINUE" BorderPane.alignment="CENTER">
               <BorderPane.margin>
                  <Insets top="40.0" />
               </BorderPane.margin>
            </Button>
         </bottom>
      </BorderPane>
  </children>
</VBox>
